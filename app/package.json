{
  "name": "metro-app-frontend",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://boywithsilverwings.github.io/metro-workshop-app",
  "dependencies": {
    "bulma": "^0.7.5",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-router": "^5.0.1",
    "react-router-dom": "^5.0.1",
    "react-scripts": "5.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "remove-build": "rimraf build",
    "deploy-demo": "gh-pages -d build",
    "update-demo": "npm run remove-build && npm run build && npm run deploy-demo"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "devDependencies": {
    "gh-pages": "^2.0.1",
    "husky": "^2.4.1",
    "prettier": "^1.18.2",
    "pretty-quick": "^1.11.0",
    "rimraf": "^2.6.3"
  }
}
